 <Presentation PresentationClass="network">
  <PageOptions Columns="3"/>
  <WindowName>Search Results</WindowName>  
  <PageTitle>Search Results ({{{rdf:RDF/rdf:Description/prns:numberOfConnections}}})</PageTitle>
  <PageBackLinkName>More Search Options</PageBackLinkName>
  <PageBackLinkURL>~/search/all</PageBackLinkURL>
  <PageSubTitle></PageSubTitle>
  <PageDescription></PageDescription> 
  <PanelList>
    <Panel type="header"  />
    <Panel type="page" />
    <Panel Type="active">
      <Module ID="MiniSearch"  />
      <Module ID="DefaultMenu"  />
    </Panel>
    <Panel Type="main" TabType="fixed" TabSort="0" >
      <Module ID="EverythingResults">        
      </Module>
    </Panel>
    <Panel Type="passive">
      <Module ID="SearchCriteria"></Module>
      <Module ID="SearchOptions"></Module>
      <Module ID="EverythingPassiveResults">
      </Module>      
    </Panel>
    <Panel type="footer" />
  </PanelList>  
</Presentation>

