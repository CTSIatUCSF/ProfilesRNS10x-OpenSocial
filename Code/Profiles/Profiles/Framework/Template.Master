<html xmlns="http://www.w3.org/1999/xhtml">
<head id="Head1" runat="server">
    <title>Profiles RNS</title>
    <asp:ContentPlaceHolder ID="head" runat="server">

        <script src="http://ajax.microsoft.com/ajax/jquery/jquery-1.4.2.js" type="text/javascript"></script>

    </asp:ContentPlaceHolder>
</head>
<form runat="server" id="form1">
<body id="bodyMaster" runat="server">
    <div id="divStatus" style="position: absolute; top: 0px; left: 0px; width: 100%;
        height: 100%; z-index: 999999; text-align: center; opacity: 0.7;display:none">
        <span style="border-width: 0px; position: absolute; padding: 50px; background-color: #FFFFFF;
            font-size: 25px; font-family:Arial; left: 40%; top: 40%;">Loading ...</span>
    </div>
    <asp:ToolkitScriptManager AsyncPostBackTimeout="36000" CombineScripts="false" ID="toolkitScriptMaster"
        runat="server">
    </asp:ToolkitScriptManager>

    <script type="text/javascript" language="javascript">

        Sys.WebForms.PageRequestManager.getInstance().add_endRequest(EndRequestHandler);

        function EndRequestHandler(sender, args) {
            if (args.get_error() != undefined) {
                args.set_errorHandled(true);
            }
        }

        function ShowStatus() {
            document.getElementById("divStatus").style.display = "block";
        }



    </script>

    <div id="header-ucsf">
      <map name="ucsfnosearch">
	 <area shape="rect" alt="UCSF home page" coords="38,3,84,27" href="http://www.ucsf.edu/" />
	 <area shape="rect" alt="UCSF home page" coords="93,11,288,19" href="http://www.ucsf.edu/" />
	 <area shape="rect" alt="About UCSF" coords="306,11,368,19" href="http://www.ucsf.edu/about_ucsf/" />
	 <area shape="rect" alt="UCSF Medical Center" coords="387,11,498,19" href="http://www.ucsfhealth.org/" />
      </map>
      <img src="http://cores.ucsf.edu/assets/img/ucsfgraynosearch.gif" alt="UCSF navigation bar" width="537" height="30" usemap="#ucsfnosearch">
    </div>

    <div class="institutionHeader">
        <div id="profiles-logo">
        <h1><a href="../">UCSF Profiles</a></h1>
        <p>search, discover, network</p>
        </div>
        <div id="powered-by-block">
          <a id="powered-by-ctsi" href="http://ctsi.ucsf.edu/" target="_blank">Powered by CTSI</a>
        </div>
    </div>

    <div id="suckerfish-container">
       <div id="suckerfishmenu">
